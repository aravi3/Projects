<h1>Signup</h1>

<form action="<%= users_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">
    
  <label for="username">Username</label>
  <input
    type="text"
    name="user[user_name]"
    id="username">
    <br><br>

  <label for="pwd">Password</label>
  <input
    type="text"
    name="user[password]"
    id="pwd">
  <br><br>

  <input type="submit" value="Sign Up">
</form>
